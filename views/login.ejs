<%- include layouts/header.ejs %>

    <div class="row">

            <form method="post" action='/controllers/authenticate-controller'>
        
            <table cellpadding="11">            
            <tr>
                <td class="label">Email</td><td>: <input type="text" name="email" required></td>
            </tr>
            <tr>
                <td class="label">Paswword</td><td>: <input type="password" name="password" required></td>
            </tr>
            <tr>
                <td class="label"></td>
                <td>
                    &nbsp; <input type="submit" name="submit" value="Sign In">
                <input type="button" name="cancel" value="Cancel" class="cancel" onClick="signUp();">
                </td>
            </tr>
            </table>
        </form>
        
    </div>
     

